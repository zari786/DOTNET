@model IEnumerable<CodeFirst.Models.Customer>

@{
    ViewBag.Title = "Pending";
    Layout = null;
}
@if (ViewBag.Message != null)
{
    <div class="alert alert-danger" style="text-align:center">
        <strong>Error!</strong>@ViewBag.Message
    </div>
}
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>SB Admin - Dashboard</title>
    <!-- Custom fonts for this template-->
    <link href="~/Admin/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <!-- Page level plugin CSS-->
    <link href="~/Admin/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet" />
    <!-- Custom styles for this template-->
    <link href="~/Admin/css/sb-admin.css" rel="stylesheet" />
</head>
<body id="page-top">

    <nav class="navbar navbar-expand navbar-dark bg-dark static-top">

        <a class="navbar-brand mr-1" href="/Admin/Index">ALN AVIATION ADMIN DASHBOARD</a>
        <div style="padding-left:900px">
            @Html.ActionLink("Logout", "Logout", "Account")
        </div>
    </nav>

    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="sidebar navbar-nav">

            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Pending Posts", "Pending", "Admin")</span>

                </a>

            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Update Status", "UpdateStatus", "Admin")</span>
                </a>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Completed Posts", "CompletedPost", "Admin")</span>
                </a>
            </li>


            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Add Schedule Flight", "AddScheduleFlight", "Admin")</span>
                </a>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Schedule Flights", "ScheduleFlights", "Admin")</span>
                </a>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Delete Schedule Flights", "DeleteScheduleFlights", "Admin")</span>
                </a>
            </li>

            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("View Customer", "ViewCustomer", "Admin")</span>
                </a>
            </li>
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="pagesDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <i class="fas fa-fw fa-folder"></i>
                    <span>@Html.ActionLink("Block Customer", "BlockCustomer", "Admin")</span>
                </a>
            </li>

        </ul>

        <div id="content-wrapper">

            <div class="container-fluid">
                <!-- Icon Cards-->
                <!-- /#wrapper -->
                <div class="pre-scrollable">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                        <div class="card">
                            <h4 class="card-header">Delete Schedule Flight</h4>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered first">
                                        <thead>
                                            <tr>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Name)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Email)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Phone)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.CompanyName)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Address)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Country)
                                                </th>
                                                <th></th>
                                            </tr>

                                            @foreach (var item in Model)
                                            {
                                                <tr>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Name)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Email)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Phone)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.CompanyName)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Address)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.Country)
                                                    </td>
                                                    <td>
                                                        @Html.ActionLink("Delete", "DeleteScheduleFlightsBool", new { id = item.CustomerId })
                                                    </td>
                                                </tr>
                                            }

                                    </table>

                                </div>
                            </div>
                        </div>


                    </div>

                </div>



            </div>
        </div>
    </div>







    <!-- /.container-fluid -->
    <!-- Sticky Footer -->
    <footer class="sticky-footer">
        <div class="container my-auto">
            <div class="copyright text-center my-auto">
                <span>Copyright AL-NASEEB AVIATION 2019</span>
            </div>
        </div>
    </footer>


    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
</body>

<!-- Bootstrap core JavaScript-->
<script src="~/Admin/vendor/jquery/jquery.min.js"></script>
<script src="~/Admin/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<!-- Core plugin JavaScript-->
<script src="~/Admin/vendor/jquery-easing/jquery.easing.min.js"></script>
<!-- Page level plugin JavaScript-->
<script src="~/Admin/vendor/chart.js/Chart.min.js"></script>
<script src="~/Admin/vendor/datatables/jquery.dataTables.js"></script>
<script src="~/Admin/vendor/datatables/dataTables.bootstrap4.js"></script>

<!-- Custom scripts for all pages-->
<script src="~/Admin/js/sb-admin.min.js"></script>

<!-- Demo scripts for this page-->
<script src="~/Admin/js/demo/datatables-demo.js"></script>
<script src="~/Admin/js/demo/chart-area-demo.js"></script>
